<?xml version="1.0" encoding="utf-8"?>
<MotionScene 
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:motion="http://schemas.android.com/apk/res-auto"
    >

    <Transition
        motion:constraintSetEnd="@+id/end"
        motion:constraintSetStart="@id/start"
        motion:duration="5000"
        motion:autoTransition="animateToEnd">
       <KeyFrameSet>
           <KeyAttribute
               motion:motionTarget="@+id/logoimg"
               motion:framePosition="0"
               android:translationY="200dp" />
           <KeyAttribute
               motion:motionTarget="@+id/logoimg"
               motion:framePosition="0"
               android:alpha="0" />
           <KeyAttribute
               motion:motionTarget="@+id/logoimg"
               motion:framePosition="0"
               android:scaleX="1.5"
               android:scaleY="1.5" />
           <KeyAttribute
               motion:motionTarget="@+id/logoimg"
               motion:framePosition="42"
               android:alpha="1"
               android:translationY="200dp" />
           <KeyAttribute
               motion:motionTarget="@+id/logoimg"
               motion:framePosition="41"
               android:scaleX="1.5"
               android:scaleY="1.5" />
           <KeyAttribute
               motion:motionTarget="@+id/register_form"
               motion:framePosition="0"
               android:alpha="0"
               android:translationY="300dp" />
           <KeyAttribute
               motion:motionTarget="@+id/register_form"
               motion:framePosition="42"
               android:alpha="0"
               android:translationY="0dp" />
           <KeyAttribute
               motion:motionTarget="@+id/titletext"
               motion:framePosition="0"
               android:alpha="0" />
           <KeyAttribute
               motion:motionTarget="@+id/logoimg"
               motion:framePosition="87"
               android:translationY="0dp" />
           <KeyAttribute
               motion:motionTarget="@+id/titletext"
               motion:framePosition="88"
               android:alpha="0" />
           <KeyAttribute
               motion:motionTarget="@+id/registerlink"
               motion:framePosition="0"
               android:alpha="0" />
           <KeyAttribute
               motion:motionTarget="@+id/registerlink"
               motion:framePosition="87"
               android:alpha="0" />
           <KeyAttribute
               motion:motionTarget="@+id/logoimg"
               motion:framePosition="87"
               android:scaleX="1"
               android:scaleY="1" />
           <KeyAttribute
               motion:motionTarget="@+id/avisolink"
               motion:framePosition="0"
               android:alpha="0" />
           <KeyAttribute
               motion:motionTarget="@+id/avisolink"
               motion:framePosition="86"
               android:alpha="0" />
           <KeyAttribute
               motion:motionTarget="@+id/register_form"
               motion:framePosition="100"
               android:translationY="0dp" />
       </KeyFrameSet>
    </Transition>

    <ConstraintSet android:id="@+id/start">
    </ConstraintSet>

    <ConstraintSet android:id="@+id/end">

    </ConstraintSet>
</MotionScene>