<?xml version="1.0" encoding="utf-8"?>
<MotionScene
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:motion="http://schemas.android.com/apk/res-auto"
    >

    <Transition
        motion:constraintSetEnd="@+id/end"
        motion:constraintSetStart="@id/start"
        motion:duration="5000"
        motion:autoTransition="animateToEnd">
        <KeyFrameSet>

            <KeyAttribute
                motion:motionTarget="@+id/logoimg"
                motion:framePosition="0"
                android:translationY="@dimen/logoposition" />
            <KeyAttribute
                motion:motionTarget="@+id/logoimg"
                motion:framePosition="46"
                android:translationY="@dimen/logoposition" />
            <KeyAttribute
                motion:motionTarget="@+id/logoimg"
                motion:framePosition="0"
                android:scaleX="1.5"
                android:scaleY="1.5"/>
            <KeyAttribute
                motion:motionTarget="@+id/logoimg"
                motion:framePosition="45"
                android:scaleX="1.5"
                android:scaleY="1.5"/>
            <KeyAttribute
                motion:motionTarget="@+id/logoimg"
                motion:framePosition="0"
                android:alpha="0" />
            <KeyAttribute
                motion:motionTarget="@+id/logoimg"
                motion:framePosition="45"
                android:alpha="1" />
            <KeyAttribute
                motion:motionTarget="@+id/viewLogin"
                motion:framePosition="0"
                android:alpha="0" />
            <KeyAttribute
                motion:motionTarget="@+id/viewLogin"
                motion:framePosition="48"
                android:alpha="0" />
            <KeyAttribute
                motion:motionTarget="@+id/logoimg"
                motion:framePosition="89"
                android:translationY="0dp" />
            <KeyAttribute
                motion:motionTarget="@+id/logoimg"
                motion:framePosition="88"
                android:scaleX="1"
                android:scaleY="1" />
            <KeyAttribute
                motion:motionTarget="@+id/viewLogin"
                motion:framePosition="48"
                android:translationY="300dp" />
            <KeyAttribute
                motion:motionTarget="@+id/logoimg"
                motion:framePosition="0"
                android:elevation="3dp" />
        </KeyFrameSet>
    </Transition>

    <ConstraintSet android:id="@+id/start">
        <Constraint
            android:id="@+id/viewLogin"
            />
    </ConstraintSet>

    <ConstraintSet android:id="@+id/end">
        <Constraint
            />
    </ConstraintSet>
</MotionScene>